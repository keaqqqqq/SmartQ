@page "/Dashboard"
<head>
    <link href="_content/MudBlazor/MudBlazor.min.css" rel="stylesheet" />
</head>

<MudCheckBox T="bool" @bind-Checked="Basic_CheckBox1"></MudCheckBox>
<MudCheckBox T="bool" @bind-Checked="Basic_CheckBox2" Color="Color.Primary"></MudCheckBox>
<MudCheckBox T="bool" @bind-Checked="Basic_CheckBox3" Color="Color.Secondary"></MudCheckBox>
<MudCheckBox T="bool" @bind-Checked="Basic_CheckBox4"></MudCheckBox>

@code {
    public bool Basic_CheckBox1 { get; set; } = true;
    public bool Basic_CheckBox2 { get; set; } = false;
    public bool Basic_CheckBox3 { get; set; } = false;
    public bool Basic_CheckBox4 { get; set; } = true;
}